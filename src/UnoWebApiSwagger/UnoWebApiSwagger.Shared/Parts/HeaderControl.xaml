<UserControl
    x:Class="UnoWebApiSwagger.Shared.Parts.HeaderControl"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="340"
    d:DesignWidth="795">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="50" />
            <ColumnDefinition Width="100" />
            <ColumnDefinition Width="100" />
            <ColumnDefinition Width="100" />
            <ColumnDefinition Width="200" />
        </Grid.ColumnDefinitions>
        <TextBlock Grid.Column="1" Text="Spot" HorizontalAlignment="Center"/>
        <TextBlock Grid.Column="2" Text="Week" HorizontalAlignment="Center" />
        <TextBlock Grid.Column="3" Text="Month" HorizontalAlignment="Center" />
    </Grid>
</UserControl>
